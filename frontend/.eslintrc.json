{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parser": "babel-eslint",
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": { "no-param-reassign": 1, "no-plusplus": 1 }
}
